       }]
    },
    "dev2": {
        "app_function": "codigoLambda2.descargacsv",
        "keep_warm": false,
        "role_name": "LabRole",
        "manage_roles": false,
        "apigateway_enabled": false,
        "aws_region": "us-east-1",
        "project_name": "parcial1_lambda2",
        "runtime": "python3.8",
        "s3_bucket": "bucketparcial1",
        "events": [{
            "function": "codigoLambda2.descargacsv",
